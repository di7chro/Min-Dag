<?xml version="1.0" encoding="utf-8"?>

<TableLayout xmlns:android="http://schemas.android.com/apk/res/android"
             android:id="@+id/tableLayout2"
             android:layout_width="fill_parent"
             android:layout_height="fill_parent">
    <RadioGroup
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:id="@+id/settingsTwitterRadio">
        <TextView
                android:id="@+id/hgdhjdgjgf"
                android:text="@string/settingsTwitterHint"
                android:layout_height="wrap_content"
                android:layout_width="fill_parent"/>


        <RadioButton
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/settingTwitterOfficialText"
                android:id="@+id/settingsTwitterEget" android:layout_column="0" android:checked="false"/>


        <RadioButton
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/settingTwitterTaggat"
                android:id="@+id/settingsTwitterEget" android:layout_column="0" android:checked="false"/>


        <RadioButton
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/settingTwitterEget"
                android:id="@+id/settingsTwitterEget" android:layout_column="0" android:checked="false"/>


        <EditText
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:id="@+id/settingTwitterEgetText" android:layout_column="1"/>

        <Button
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="OK"
                android:id="@+id/settingTwitterOKButton" android:layout_column="1"/>
    </RadioGroup>
</TableLayout>

